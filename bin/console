#!/usr/bin/env php
<?php
require_once __DIR__ . '/../vendor/autoload.php';

use Kathamo\App\Commands\CreateTemplateCommand;
use Symfony\Component\Console\Application;
use Kathamo\App\Commands\ScaffoldCommand;

$app = new Application();
